#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/usr/bin/env" ]; then
  exec "$basedir/usr/bin/env" node "$basedir/../mickeldav/index.js" "$@"
else 
  exec usr/bin/env node "$basedir/../mickeldav/index.js" "$@"
fi
